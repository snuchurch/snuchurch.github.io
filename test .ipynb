      "                        var month = response[i].Month;\n",
      "                        var day = response[i].Day;\n",
      "\n",
      "                        selectDate = moment(new Date(year, (month - 1), day)).format('YYYY-MM-DD');\n",
      "                    }\n",
      "\n",
      "                    if (response[i].Cell_num % 7 == 1){\n",
      "                        str = str + \"<tr>\";\n",
      "                    }\n",
      "\n",
      "                    if (response[i].Day != \"\") {\n",
      "                        str = str + \"<td><a id='\" + selectDate + \"' onclick=SelectNextCheck('\" + selectDate + \"')> <div class='\" + week + \"'>\" + response[i].Day + \"</div><div class=bible_cont>\" + response[i].Bible_name + \"<br>\" + response[i].Bible_chapter + \"</div></a></td>\";\n",
      "                    }\n",
      "                    else {\n",
      "                        str = str + \"<td><div class='\" + week + \"'></div><div class=bible_cont><br></div></td>\";\n",
      "                    }\n",
      "\n",
      "                    if (response[i].Cell_num % 7 == 1 && count == 7){\n",
      "                        str = str + \"<tr>\";\n",
      "                        count = 0;\n",
      "                    }\n",
      "                };\n",
