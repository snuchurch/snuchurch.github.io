      "            dataType: 'JSON',\n",
      "            success: function (response) {\n",
      "                var count = 0;\n",
      "                $('#bibleCalendar').html(\"\");\n",
      "                var str = \"\";\n",
      "                for (var i = 0; i < response.length; i++) {\n",
      "\n",
      "                    var bible_date = response[i].Bible_date;\n",
      "\n",
      "                    if (response[i].Weeks > 1 && response[i].Week == 1 && bible_date == \"\") {\n",
      "                        break;\n",
      "                    } else {\n",
      "                        count++;\n",
      "                    }\n",
      "\n",
      "                    var week = response[i].Week == 1 ? \"sunday\" : (response[i].Week == 7 ? \"saturday\" : \"date\");\n",
      "                    var selectDate;\n",
      "\n",
      "                    if (response[i].Day != \"\") {\n",
